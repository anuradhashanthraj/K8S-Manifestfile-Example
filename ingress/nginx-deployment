apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-deployment
spec:
  replicas: 3
  selector:
    matchLabels:
      app: nginx-web
  template:
    metadata:
      labels:
        app: nginx-web
    spec:
      containers:
        - name: nginx-container
          image: nginx
          ports: 
            - containerPort: 80
          env:
            - name: TITLE
              value: "NGINX"
---
apiVersion: v1
kind: Service 
metadata:
  name: nginx-svc
spec:
  selector:
    app: nigix-web
  ports:
    - port: 86
      targetPort: 80
      protocol: TCP
  type: ClusterIP        


  


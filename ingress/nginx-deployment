apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-deployment
spec:
  replicas: 3
  selector:
    matchLabels:
      app: nginx-web
  template:
    metadata:
      labels:
        app: nginx-web
    spec:
      containers:
        - name: nginx-container
          image: nginx
          ports: 
            - containerPort: 80
          env:
            - name: TITLE
              value: "NGINX"
---
apiVersion: v1
kind: Service
metadata:
  name: nginx-deployment
spec:
  selector:
    app: nginx-web
  ports:
    - port: 80
      protocal: TCP
      tagetPort: 80
  type: ClusterIP
  

